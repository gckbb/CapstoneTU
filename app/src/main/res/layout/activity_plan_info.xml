<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"
        tools:context=".Map.PlanInfoActivity">
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        tools:ignore="MissingConstraints">


        <ImageButton
            android:id="@+id/back_btn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="23dp"
            android:background="@drawable/ic_back"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />




        <Button
            android:id="@+id/nextbutton"
            android:layout_width="83dp"
            android:layout_height="47dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="250dp"
            android:text="다음"
            android:background="@drawable/buttonshape1"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="18dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="TouchTargetSizeCheck" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="34dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="어디로? "
            android:textColor="@color/black"
            android:textSize="18dp"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"

            />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/where"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:text=""
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginTop="11dp" />

            <ImageView
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text=""
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginTop="11dp" />
        </FrameLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:paddingLeft="34dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="언제?"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            tools:ignore="MissingConstraints"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">


            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="40dp"
                    android:background="@drawable/edittext3"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                    tools:layout_marginTop="11dp"/>

                <TextView
                    android:id="@+id/day1txt"
                    android:layout_width="143dp"
                    android:layout_height="40dp"
                    android:text=""
                    android:textSize="20dp"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                    tools:layout_marginTop="11dp"/>

                <Button
                    android:id="@+id/day1btn"
                    android:layout_width="23dp"
                    android:layout_height="23dp"
                    android:background="@drawable/calendar"
                    android:text=""
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                    tools:layout_marginLeft="125dp"
                    tools:layout_marginTop="20dp"
                    />
            </FrameLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="~"
                tools:layout_marginLeft="5dp"
                tools:layout_marginTop="10dp"
                android:textSize="27dp"
                android:textStyle="bold"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints"/>
            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="160dp"
                    android:layout_height="40dp"
                    android:background="@drawable/edittext3"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                    tools:layout_marginLeft="5dp"
                    tools:layout_marginTop="11dp"/>

                <TextView
                    android:id="@+id/day2txt"
                    android:layout_width="143dp"
                    android:layout_height="40dp"
                    android:text=""
                    android:textSize="20dp"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                    tools:layout_marginLeft="5dp"
                    tools:layout_marginTop="11dp"/>

                <Button
                    android:id="@+id/day2btn"
                    android:layout_width="23dp"
                    android:layout_height="23dp"
                    android:background="@drawable/calendar"
                    android:text=""
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                    tools:layout_marginLeft="130dp"
                    tools:layout_marginTop="20dp" />
            </FrameLayout>

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:paddingLeft="34dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="누구와?"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            tools:ignore="MissingConstraints"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">


            <Button
                android:id="@+id/friend"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="친구"
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                />


            <Button
                android:id="@+id/family"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="가족"
                android:textSize="18dp"
                android:textColor="@color/black"
                android:layout_marginLeft="20dp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"

                />

            <Button
                android:id="@+id/couple"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="애인"
                android:textColor="@color/black"
                android:textSize="18dp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                android:layout_marginLeft="20dp"
                />

            <Button
                android:id="@+id/alone"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="혼자"
                android:textColor="@color/black"
                android:textSize="18dp"

                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                android:layout_marginLeft="20dp"
                />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:paddingLeft="34dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="이동수단은?"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            tools:ignore="MissingConstraints"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"

            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <Button
                android:id="@+id/car"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="자차"
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                />
            <Button
                android:id="@+id/bus"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="버스"
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginLeft="20dp"
                />

            <Button
                android:id="@+id/taxi"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="택시"
                android:textColor="@color/black"
                android:textSize="18dp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginLeft="20dp"
                />

            <Button
                android:id="@+id/walk"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text="도보"
                android:textColor="@color/black"
                android:textSize="18dp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginLeft="20dp"
                />
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:paddingLeft="34dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">
        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="테마?"

            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            tools:ignore="MissingConstraints"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"

            android:orientation="horizontal"
            tools:ignore="MissingConstraints">
            <Button
                android:id="@+id/theme1"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text=""
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                android:clickable="false"/>


            <Button
                android:id="@+id/theme2"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text=""
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginLeft="20dp"
                />

            <Button
                android:id="@+id/theme3"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text=""
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginLeft="20dp" />

            <Button
                android:id="@+id/theme4"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:background="@drawable/edittext3"
                android:text=""
                android:textSize="18dp"
                android:textColor="@color/black"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                tools:layout_marginLeft="20dp"
                />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:paddingLeft="34dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">
    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="활동량은?"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"

        android:orientation="horizontal"
        tools:ignore="MissingConstraints">

    <Button
        android:id="@+id/low_activity"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:background="@drawable/edittext3"
        android:text="적음"
        android:textSize="18dp"
        android:textColor="@color/black"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        />


    <Button
        android:id="@+id/normal_activity"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:background="@drawable/edittext3"
        android:text="보통"
        android:textSize="18dp"
        android:textColor="@color/black"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_marginLeft="20dp"
        />

    <Button
        android:id="@+id/hard_activity"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:background="@drawable/edittext3"
        android:text="많음"
        android:textSize="18dp"
        android:textColor="@color/black"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_marginLeft="20dp" />
    </LinearLayout></LinearLayout></LinearLayout>