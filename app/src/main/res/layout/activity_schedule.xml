            android:layout_height="37dp"
            android:background="@drawable/middle"/>

        <Button
            android:id="@+id/day7"
            android:layout_width="47dp"
            android:layout_height="37dp"
            android:background="@drawable/middle"/>

        <Button
            android:id="@+id/day8"
            android:layout_width="41dp"
            android:layout_height="37dp"
            android:background="@drawable/last"/>

    </LinearLayout>
    </FrameLayout>


