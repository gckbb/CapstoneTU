            android:layout_height="25dp"
            android:background="@drawable/buttonshape2"
            android:layout_marginLeft="8dp"
            android:text="체크리스트" />


        <Button
            android:id="@+id/cash"
            android:layout_width="68dp"
            android:layout_height="25dp"
            android:background="@drawable/buttonshape2"
            android:text="가계부"
            android:layout_marginLeft="8dp"
            />

        <Button
